{
  "name": "mercuryfront",
  "version": "0.0.1",
  "author": "CodeDoesGood",
  "description": "Merucry webapp front",
  "scripts": {
    "build:hot": "webpack-dev-server --content-base ./public -d --hot --history-api-fallback true --config ./webpack.config.development.js",
    "build:dev": "webpack -d --watch --config ./webpack.config.development.js",
    "build": "npm run lint && webpack -p --progress --env.production --config ./webpack.config.production.js",
    "lint": "eslint -c .eslintrc.js --ext .js,.jsx --ignore-path .gitignore .",
    "lint:fix": "eslint -c .eslintrc.js --ext .js,.jsx --ignore-path .gitignore . --fix",
    "start:pm2": "pm2 start ./public/server.js --name Mercury:Frontend"
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "less": "^3.0.1",
    "lodash": "^4.17.5",
    "material-ui": "^1.0.0-beta.37",
    "material-ui-icons": "^1.0.0-beta.36",
    "npm": "^5.7.1",
    "pm2": "^2.10.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "superagent": "^3.8.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-sort-requires": "^2.1.0",
    "less-loader": "^4.1.0",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.1"
  }
}
