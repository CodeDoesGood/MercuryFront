{
  "name": "mercuryfront",
  "version": "0.0.1",
  "author": "CodeDoesGood",
  "description": "Merucry webapp front",
  "scripts": {
    "build:hot": "webpack-dev-server -d --config ./webpack.config.development.js",
    "build:dev": "webpack -d --watch --config ./webpack.config.development.js",
    "build": "npm run lint && webpack -p --progress --env.production --config ./webpack.config.production.js",
    "lint": "eslint -c ./.eslintrc.js --ext .js,.jsx --ignore-path ../../.gitignore .",
    "lint:fix": "eslint -c ./.eslintrc.js --ext .js,.jsx --ignore-path ../../.gitignore . --fix"
  },
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-hash-link": "^1.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "superagent": "^3.8.3",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-sort-requires": "^2.1.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.1"
  }
}
