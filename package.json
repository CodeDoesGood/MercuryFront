{
    "name": "mercury-web-front",
    "version": "0.0.1",
    "author": "CodeDoesGood",
    "description": "Merucry webapp front",
    "scripts": {
        "build": "webpack --config ./webpack.config.production.js",
        "build:dev": "webpack-dashboard -- webpack-dev-server --content-base ./public -d --hot --history-api-fallback true --config ./webpack.config.development.js",
        "lint": "eslint -c .eslintrc.js --ext .js,.jsx --ignore-path .gitignore .",
        "lint:fix": "eslint -c .eslintrc.js --ext .js,.jsx --ignore-path .gitignore . --fix",
        "start:pm2": "pm2 start ./public/server.js --name Mercury:Frontend"
    },
    "license": "MIT",
    "dependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "bluebird": "^3.5.1",
        "body-parser": "^1.18.2",
        "css-loader": "^0.28.7",
        "express": "^4.16.2",
        "less": "^2.7.3",
        "less-loader": "^4.0.5",
        "lodash": "^4.17.4",
        "pm2": "^2.9.1",
        "prop-types": "^15.6.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.8",
        "redux": "^3.7.2",
        "style-loader": "^0.18.2",
        "superagent": "^3.8.1"
    },
    "devDependencies": {
        "babel-eslint": "^8.0.3",
        "eslint": "^4.12.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.5.1",
        "eslint-plugin-sort-requires": "^2.1.0",
        "webpack": "^3.10.0",
        "webpack-dashboard": "^1.0.2",
        "webpack-dev-server": "^2.9.5"
    }
}
